{
   "manifest_version": 2,
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "script.js" ],
      "matches": [ "http://*/*", "https://*/*", "file://*" ],
      "run_at": "document_idle"
   } ],
   "background": {
      "scripts": ["backgroundScript.js"]
   },
   "converted_from_user_script": true,
   "description": "nicokara autoloader",
   "name": "nicokaraExtension.user.js",
   "version": "1.0",
   "permissions": [ "tabs" ]
}
